{
  "name": "synqlite-web",
  "type": "module",
  "source": "src/index.ts",
  "exports": {
    "types": "./dist/index.d.ts",
    "import": "./dist/synqlite.module.js",
    "require": "./dist/synqlite.cjs",
    "default": "./dist/synqlite.modern.js"
  },
  "types": "./dist/index.d.ts",
  "main": "./dist/synqlite.cjs",
  "umd:main": "./dist/synqlite.umd.js",
  "module": "./dist/synqlite.module.js",
  "unpkg": "./dist/synqlite.umd.js",
  "scripts": {
    "build": "microbundle && rm -f dist/full-build",
    "build:full": "touch dist/full-build && microbundle --external none",
    "watch": "microbundle watch",
    "test-server": "cd tests/browser && http-server --cors -p 8181",
    "test": "playwright test",
    "test:build": "npm run build:full && npm test",
    "test:ui": "playwright test --ui",
    "links": "chmod +x tests/test-setup.sh && tests/test-setup.sh",
    "prepare": "husky"
  },
  "devDependencies": {
    "@playwright/test": "^1.41.2",
    "@types/better-sqlite3": "^7.6.9",
    "@types/bun": "latest",
    "@types/node": "^20.11.17",
    "@vitest/browser": "^1.2.2",
    "http-server": "^14.1.1",
    "microbundle": "^0.15.1",
    "vitest": "^1.2.2"
  },
  "peerDependencies": {
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "@sqlite.org/sqlite-wasm": "3.45.1-build1",
    "better-sqlite3": "^9.4.0",
    "date-fns": "^3.3.1",
    "nanoid": "^5.0.5",
    "short-unique-id": "^5.0.3",
    "tslog": "^4.9.2"
  }
}
